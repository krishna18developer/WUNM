<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ic_background"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/phone_box"
        android:layout_width="893px"
        android:layout_height="61dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="235dp"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="435dp"
        android:background="@drawable/ic_phonefield"
        android:ems="10"
        android:importantForAutofill="no"
        android:inputType="phone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="LabelFor" />

    <com.hbb20.CountryCodePicker
        android:id="@+id/ccp"
        android:layout_width="140dp"
        android:layout_height="36dp"
        android:layout_marginStart="230dp"
        android:layout_marginTop="138dp"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="557dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.507" />

    <ImageView
        android:id="@+id/send_btn"
        android:layout_width="511px"
        android:layout_height="450px"
        android:layout_marginStart="110dp"
        android:layout_marginTop="376dp"
        android:layout_marginEnd="107dp"
        android:layout_marginBottom="184dp"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_message"
        tools:layout_editor_AbsoluteX="272.45px" />

    <TextView
        android:id="@+id/dbkt_img"
        android:layout_width="311dp"
        android:layout_height="40dp"
        android:layout_marginStart="59dp"
        android:layout_marginTop="646dp"
        android:layout_marginEnd="41dp"
        android:layout_marginBottom="45dp"
        android:fontFamily="cursive"
        android:text="Developed By Krishna Teja"
        android:textColor="@color/green_home"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/sc_img"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="138dp"
        android:layout_marginEnd="221dp"
        android:layout_marginBottom="556dp"
        android:fontFamily="cursive"
        android:text="Select Country:"
        android:textColor="@color/green_home"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/wunm_txt"
        android:layout_width="155dp"
        android:layout_height="68dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="128dp"
        android:layout_marginBottom="647dp"
        android:fontFamily="cursive"
        android:text="WUNM"
        android:textColor="@color/green_home"
        android:textSize="50dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>